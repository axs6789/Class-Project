<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>
<table>
	<tr>
		<td>name</td>
		<td>deadline</td>
		<td>category</td>
		<td>user</td>
	</tr>
	<% @tasks.each do |task| %>
	<tr>
		<td><%= task.name %></td>
		<td><%= task.deadline %></td>
		<td><%= task.category %></td>
		<td><%= task.user.username unless task.user.nil?%></td>
	</tr>
	<% end %>
</table>